{
    "activationEvents": [
        "onLanguage:htmlx"
    ],
    "author": "HitalloExiled",
    "browser": "./client/browser.js",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "htmlx",
                "path": "./syntaxes/htmlx.tmLanguage.json",
                "scopeName": "text.htmlx"
            },
            {
                "injectTo": [
                    "text.htmlx"
                ],
                "path": "./syntaxes/htmlx-attribute.tmLanguage.json",
                "scopeName": "text.htmlx.attribute"
            },
            {
                "injectTo": [
                    "text.htmlx"
                ],
                "path": "./syntaxes/htmlx-custom-element.tmLanguage.json",
                "scopeName": "text.htmlx.custom-element"
            },
            {
                "injectTo": [
                    "text.htmlx"
                ],
                "path": "./syntaxes/htmlx-directive.tmLanguage.json",
                "scopeName": "text.htmlx.directive"
            },
            {
                "injectTo": [
                    "text.htmlx"
                ],
                "path": "./syntaxes/htmlx-interpolation.tmLanguage.json",
                "scopeName": "text.htmlx.interpolation"
            }
        ],
        "languages": [
            {
                "aliases": [
                    "HTMLX",
                    "htmlx"
                ],
                "configuration": "./language-configuration.json",
                "extensions": [
                    ".htmlx"
                ],
                "id": "htmlx"
            }
        ]
    },
    "description": "Htmlx Language Features",
    "devDependencies": {
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "eslint": "^8.11.0",
        "eslint-plugin-import": "^2.25.4",
        "mocha": "^9.2.2",
        "nyc": "^15.1.0",
        "typescript": "^4.6.2"
    },
    "displayName": "htmlx-language-features",
    "engines": {
        "vscode": "^1.64.0"
    },
    "main": "./client/node.js",
    "name": "htmlx-language-features",
    "scripts": {
        "cover": "nyc npm run test",
        "test": "mocha **/tests/**/*.spec.js"
    },
    "version": "0.1.0",
    "workspaces": [
        "./client",
        "./server"
    ]
}